{
    "$schema": "https:\/\/docs.renovatebot.com\/renovate-schema.json",
    "packageRules": [
        {
            "matchManagers": [
                "github-actions"
            ],
            "matchPaths": [
                ".github\/workflows\/**"
            ],
            "matchFiles": [
                "etc\/workflow-skeleton\/ci-{{ path }}.yaml",
                "etc\/workflow-skeleton\/generate-{{ path }}.yaml"
            ],
            "branchPrefix": "renovate\/github-actions\/",
            "commitMessagePrefix": "[\ud83d\udc19]",
            "labels": [
                "\ud83d\udc19"
            ]
        },
        {
            "matchManagers": [
                "composer"
            ],
            "rangeStrategy": "in-range-only",
            "matchFiles": [
                "composer.json"
            ],
            "branchPrefix": "renovate\/root\/",
            "commitMessagePrefix": "[\ud83e\udeb5]",
            "labels": [
                "\ud83e\udeb5"
            ]
        },
        {
            "matchManagers": [
                "composer"
            ],
            "rangeStrategy": "bump",
            "matchFiles": [
                "composer.json"
            ],
            "branchPrefix": "renovate\/root\/",
            "commitMessagePrefix": "[\ud83e\udeb5]",
            "labels": [
                "\ud83e\udeb5"
            ]
        },
        {
            "matchManagers": [
                "composer"
            ],
            "matchPackageNames": [
                "php"
            ],
            "enabled": false,
            "matchFiles": [
                "composer.json"
            ],
            "branchPrefix": "renovate\/root\/",
            "commitMessagePrefix": "[\ud83e\udeb5]",
